{
    "name": "ikoncept/fabriq",
    "description": "A CMS Framework by Ikoncept",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ikoncept\\Fabriq\\": "src/",
            "Ikoncept\\Fabriq\\Database\\Factories\\": "database/factories",
            "Ikoncept\\Fabriq\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {"Ikoncept\\Fabriq\\Tests\\": "tests"}
    },
    "authors": [
        {
            "name": "Albin Nilsson",
            "email": "albin@ikoncept.se"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.56",
        "illuminate/support": "^7.0.5|^8.0",
        "spatie/laravel-permission": "^4.3",
        "infab/core": "^2.1",
        "spatie/laravel-query-builder": "^3.5",
        "spatie/laravel-medialibrary": "^9.7",
        "doctrine/dbal": "^3.1",
        "kalnoy/nestedset": "^6.0",
        "spatie/laravel-tags": "^3.1",
        "spatie/laravel-sluggable": "^2.6",
        "infab/translatable-revisions": "^1.0",
        "laravel/fortify": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "^6.4",
        "nunomaduro/collision": "^5.1",
        "orchestra/database": "^6.0",
        "nunomaduro/larastan": "^0.7.12"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ikoncept\\Fabriq\\FabriqCoreServiceProvider"
            ]
        }
    },
    "repositories": {
        "0": {
            "type": "vcs",
            "url": "git@github.com:ikoncept/translatable-revisions.git"
        }
    },
    "scripts": {
        "test": [
            "php --version && php -d memory_limit=512M ./vendor/bin/phpstan analyse -c phpstan.neon",
            "php --version && vendor/bin/phpunit --coverage-text --colors=never --testdox"
        ]
    }
}
