{
    "name": "ikoncept/fabriq",
    "description": "A CMS Framework by Ikoncept",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ikoncept\\Fabriq\\": "src/",
            "Ikoncept\\Fabriq\\Database\\Factories\\": "database/factories",
            "Ikoncept\\Fabriq\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {"Ikoncept\\Fabriq\\Tests\\": "tests"}
    },
    "authors": [
        {
            "name": "Albin Nilsson",
            "email": "albin@ikoncept.se"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.56|^9.0",
        "illuminate/support": "^7.0.5|^8.0|^9.0",
        "spatie/laravel-permission": "^4.3|^5.0",
        "infab/core": "^2.1",
        "spatie/laravel-query-builder": "^3.5|^4.0|^5.0",
        "spatie/laravel-medialibrary": "^8.2|^9.0",
        "doctrine/dbal": "^3.1",
        "kalnoy/nestedset": "^6.0",
        "spatie/laravel-tags": "^3.1|^4.0",
        "spatie/laravel-sluggable": "^2.6|^3.0",
        "infab/translatable-revisions": "^1.0",
        "laravel/fortify": "^1.8",
        "dyrynda/laravel-make-user": "^7.3",
        "php-ffmpeg/php-ffmpeg": "^1.0",
        "spatie/pdf-to-image": "^2.1",
        "spatie/once": "^2.0|^3.0",
        "pusher/pusher-php-server": "^7.0",
        "aws/aws-sdk-php": "^3.219",
        "laravel/pint": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "^7.0",
        "nunomaduro/collision": "^5.1|^6.0",
        "orchestra/database": "^7.0",
        "nunomaduro/larastan": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ikoncept\\Fabriq\\FabriqCoreServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "php --version && php -d memory_limit=512M ./vendor/bin/phpstan analyse -c phpstan.neon",
            "php --version && vendor/bin/phpunit --coverage-text --colors=never --testdox"
        ]
    }
}
